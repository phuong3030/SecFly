<table border="1">
<tr>
<th>ID</th>
<th>From</th>
<th>To</th>
<th>Created at</th>
<th>Depart</th>
<th>Depart time</th>
<th>Return</th>
<th>Return time</th>
<th>Adult tickets</th>
<th>Children (2 -> 12 yrs)</th>
<th>Infant (< 2 yrs)</th>
<th>Payment_method</th>
</tr>
<%= @orders.each do |order| %>
	<tr>
	<td><%= order.id %></td>
	<td><%= order.from %></td>
	<td><%= order.to %></td>
	<td><%= order.created_at %></td>
	<td><%= order.depart_date %></td>
	<td><%= order.depart_time_slot %></td>
	<td><%= order.return_date %></td>
	<td><%= order.return_time_slot %></td>
	<td><%= order.adult_names %></td>
	<td><%= order.children_names %></td>
	<td><%= order.infant_names %></td>
	<td><%= order.payment_method %></td>
	</tr>
<% end %>
</table>
