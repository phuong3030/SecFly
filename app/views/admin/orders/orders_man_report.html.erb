<% content_for :page_spec do %>
	<title>Customer orders</title>
	<%= stylesheet_link_tag "admin/dashboard", media: "all" %>
	<%= stylesheet_link_tag "jquery-ui-custom", media: "all" %>
	<%= stylesheet_link_tag "admin/order_report", media: "all" %>
<% end %>

<% content_for :page_script do %>
	<%= javascript_include_tag "admin/panel_nav_toggle" %>
	<%= javascript_include_tag "jquery-ui-custom" %>
	<%= javascript_include_tag "admin/order_report" %>
<% end %>

<% content_for :breadcrumbs do %>
	<%= create_breadcrumbs(
		[ 
			{ :name => 'Orders', :path => admin_orders_path }, 
			{ :name => 'Orders reporting services' }
		]	
	)%>
<% end %>

<% content_for :pageheader do %>
	<div class='page-icon'>
		<i class='fa fa-money fa-3x'></i>
	</div>
	<div class='page-title'>
		<h6>ORDERS REPORTING</h6>
		<h1>Orders</h1>
	</div>
<% end %>

<% content_for :maincontent do %>
	<%= render 'admin/orders/shared/export_filter_form' %>
<% end %>

<%= render :partial => 'shared/admin/cms_master_with_nav'  %>
