<% content_for :page_spec do %>
	<title>Resources management</title>
	<%= stylesheet_link_tag "admin/dashboard", media: "all" %>
<% end %>

<% content_for :page_script do %>
	<%= javascript_include_tag "admin/panel_nav_toggle" %>
<% end %>

<% content_for :breadcrumbs do %>
	<%= create_breadcrumbs(
		[ { :name => 'Resources', :path => admin_resources_path }, { :name => 'New' } ]		
	)%>
<% end %>

<%= content_for :pageheader do %>
	<div class='page-icon'>
		<i class='fa fa-laptop fa-3x'></i>
	</div>
	<div class='page-title'>
		<h6>NEW RESOURCES</h6>
		<h1>Resources</h1>
	</div>
<% end %>

<% content_for :maincontent do %>

	<%= render 'form', :url => admin_resources_path, :method => 'post' %>

	<%= link_to 'Back', admin_resources_path %>

<% end %>

<%= render :partial => 'shared/admin/cms_master_with_nav' %>
